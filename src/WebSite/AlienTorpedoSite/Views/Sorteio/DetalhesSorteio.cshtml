@model List<AlienTorpedoSite.Models.Grupo.GrupoEvento>

@{
    <!-- Informando layout a ser utilizado caso o user esteja logado -->
    Layout = "_Layout_logado";
}

<table>
    <tr>
        <th>IdGrupoEvento</th>
        <th>CdGrupo</th>
        <th>DtEvento</th>
        <th>NmEvento</th>
        <th>NmEndereco</th>
        <th>VlEvento</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.IdGrupoEvento</td>
            <td>@item.CdGrupo</td>
            <td>@item.DtEvento</td>
            <td>@item.NmEvento</td>
            <td>@item.NmEndereco</td>
            <td>@item.VlEvento</td>
        </tr>
    }
</table>